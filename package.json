{
  "name": "truly",
  "version": "0.20.0",
  "description": "Check if the value is not undefined, null, empty string, NaN and Infinity.",
  "main": "truly.js",
  "support": "truly.support.js",
  "deploy": "truly.deploy.js",
  "browser": "truly.deploy.js",
  "scripts": {
	"pristine": "rsetmod clean && flxc execute ./.install.sh",
	"clear": "rsetmod clean --clear",
	"formal": "jesy apply '*.json'",
	"build:support": "gulp --module truly --all",
	"build:deploy": "webpack --config webpack.compact.js --progress --display-modules",
	"rebuild": "npm run build:support && npm run build:deploy",
	"build": "npm run pristine && npm run rebuild",
	"test:server": "mocha test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/volkovasystems/truly.git"
  },
  "keywords": [
    "value",
    "truthy",
    "true",
    "truth",
    "truly"
  ],
  "author": "Richeve S. Bebedor <richeve.bebedor@gmail.com>",
  "contributors": [
  	"John Lenon Maghanoy <johnlenonmaghanoy@gmail.com>",
  	"Vinse Vinalon <vinsevinalon@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/volkovasystems/truly/issues"
  },
  "homepage": "https://github.com/volkovasystems/truly#readme",
  "dependencies": {
    "babel-runtime": "^6.23.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "source-map-loader": "^0.2.1",
    "webpack": "^2.6.1"
  }
}
